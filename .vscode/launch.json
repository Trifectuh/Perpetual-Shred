{
	"version": "0.2.0",
	"configurations": [
		{
			"type": "chrome",
			"request": "launch",
			"name": "Chrome",
			"url": "http://localhost:4200",
			"webRoot": "${workspaceRoot}/frontend",
			"breakOnLoad": true
		},
		{
			"name": "Debug Frontend",
			"type": "chrome",
			"request": "launch",
			"url": "http://localhost:4200",
			"webRoot": "${workspaceFolder}/frontend"
		},
		{
			"type": "node",
			"request": "launch",
			"cwd": "${workspaceFolder}/backend",
			"name": "Debug Backend",
			"runtimeExecutable": "npm",
			"runtimeArgs": [
				"run",
				"dev"
			],
			"port": 9229
		},
	]
}